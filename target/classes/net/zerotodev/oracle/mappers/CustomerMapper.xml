<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.zerotodev.oracle.mappers.CustomerMapper">
	<insert id="save" parameterType="net.zerotodev.oracle.customer.domain.CustomerDto">
		insert into 
		customers (cust_id, cust_name, address, phone)
		values (${custId}, #{custName}, #{address}, #{phone})
	</insert>
	<select id="findById" resultType="string"  parameterType="string"></select>
	<select id="findAll" resultType="string" ></select>
	<update id="update" parameterType="string"></update>
	<delete id="delete" parameterType="string"></delete>
</mapper>