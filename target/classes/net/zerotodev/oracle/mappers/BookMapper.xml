<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.zerotodev.oracle.mappers.BookMapper">
	<select id="findAll" resultType="net.zerotodev.oracle.book.domain.BookDto">
		select 
			book_id bookId,
			book_title bookTitle,
			price,
			pub_id pubId
		 from books
	</select>
	
</mapper>