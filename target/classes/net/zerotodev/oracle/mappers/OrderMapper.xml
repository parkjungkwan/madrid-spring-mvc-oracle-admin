<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.zerotodev.oracle.mappers.OrderMapper">
	<insert id="save" parameterType="net.zerotodev.oracle.orders.domain.OrderDto">
		insert into
		orders (order_id,cust_id,book_id,order_price,order_date)
		values (${orderId},${custId},${bookId},${orderPrice},#{orderDate})
	</insert>

	<select id="findById" resultType="string"  parameterType="string"></select>
	<select id="findAll" resultType="string" ></select>
	<update id="update" parameterType="string"></update>
	<delete id="delete" parameterType="string"></delete>
</mapper>